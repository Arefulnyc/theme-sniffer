
{
  "name": "theme-sniffer",
  "version": "0.2.0",
  "description": "Theme Sniffer plugin using sniffs. `WordPress-Theme` standard is used from WPTRT/WordPress-Coding-Standards.",
  "main": "",
  "browserslist": [
    "android >= 4.2",
    "not ie < 11",
    "last 2 versions"
  ],
  "dependencies": {
    "eslint-config-wordpress": "^2.0.0",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "archiver": "^3.0.0",
    "autoprefixer": "^9.3.1",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.1.4",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-config-wordpress": "^13.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "precommit": "eslint assets/dev/scripts/*.js && stylelint \"assets/dev/styles/**/*.scss\" --syntax scss && ./vendor/bin/phpcs --ignore=*/vendor/*,*/node_modules/* .",
    "start": "webpack --progress --watch --display-error-details --display-modules --display-reasons",
    "build": "NODE_ENV=production webpack -p --progress"
  },
